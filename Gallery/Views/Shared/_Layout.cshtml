<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewBag.Title</title>
    @System.Web.Optimization.Styles.Render("~/Content/css")
    @System.Web.Optimization.Scripts.Render("~/bundles/modernize")
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Home Page", "Index", "Home")</li>
                    <li>@Html.ActionLink("Upload Image", "Upload", "Home")</li>
                </ul>
                <ul class="nav navbar-right nav-tabs">
                    <li>@Html.ActionLink("Sign up", "Index", "Auth")</li>
                    <li><a onclick="vis()">Log in</a></li>
                </ul>
            </div>
        </div>
    </div>
    <div id="ok" class="container body-content">
        @RenderBody()
    </div>
    <div class="container body-content">
        <hr />
        <footer>
            <p>&copy; 2020 - Asp.Net Project</p>
        </footer>
    </div>
    @System.Web.Optimization.Scripts.Render("~/bundles/jquery")
    @System.Web.Optimization.Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>

<script>
    function vis() {
        $("#ok").append(
            "<div class='login-layer'>" +
            "\n\t<div class='layer-box'>" +
            "\n\t\t<div class='login'>" +
            "\n\t\t\t<div>" +
            "\n\t\t\t\t<div class='login-input-wrapper'>" +
            "\n\t\t\t\t\t<label for='login-email'>Email</label>" +
            "\n\t\t\t\t\t<input class='login-input' id='login-email' name='username' type='text' placeholder='Email'/>" +
            "\n\t\t\t\t</div>" +
            "\n\t\t\t\t<div class='login-input-wrapper'>" +
            "\n\t\t\t\t\t<label for='login-password'>Password</label>" +
            "\n\t\t\t\t\t<input class='login-input' id='login-password' name='password' type='password' placeholder='Password'/>" +
            "\n\t\t\t\t</div>" +
            "\n\t\t\t</div>" +
            "\n\t\t\t<div style='width:35%;'>" +
            "\n\t\t\t<img style='position:relative;left:10%;' src='data:image/jpeg;base64,'/>" +
            "\n\t\t\t<div>" +
            "\n\t\t</div>" +
            "\n\t</div>" +
            "\n</div>"
        );
        $(".layer-box").fadeIn(100);
        $(".login-layer").click(function() {
            $(".login-layer").fadeOut(100);
            setTimeout(() => { $(".layer-box").remove(); },100);
        });
    }
</script>
<style>
    .login-input-wrapper {
        position: relative;
        margin-bottom: 1rem;
    }

    .login-input {
        box-sizing: border-box;
        width: 100%;
        padding: .8rem 1rem;
        font-size: 1.2rem;
        line-height: 2rem;
        border: 1px solid #aaa;
        border-radius: 0.4rem;
        background-clip: padding-box;
        height: 3.8rem;
    }

    .login-layer {
        top: 6.4rem;
        padding: 0 2rem 2rem;
        height: 28rem;
        box-shadow: none;
        box-sizing: border-box;
        left: 0;
        z-index: 1;
        width: 100%;
        transition: top 0.3s ease-in-out 0s;
        position: absolute;
        overflow: hidden;
    }

    .layer-box {
        padding: 0;
        box-shadow: 0 0 1rem rgba(0, 0, 0, 0.25);
        border: 1px solid #bdc2ca;
        border-top: 0;
        border-radius: 0 0 0.4rem 0.4rem;
        background-clip: padding-box;
        box-sizing: border-box;
        overflow: hidden;
        position: relative;
        background-color: #fff;
        width: 32%;
        height: 74%;
        float: right;
    }

    .login {
        align-items: center;
        justify-content: center;
        display: flex;
        height: 100%;
        width: 31rem;
        border: 0;
        padding: 1rem 2rem 2rem;
        border-radius: 0 0 0.4rem 0.4rem;
        background-clip: padding-box;
        background-color: #fff;
        box-sizing: border-box;
        position: relative;
    }
</style>
